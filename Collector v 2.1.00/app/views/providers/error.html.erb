<title>Create new Provider Error</title>

<table class="background">
  <tr class="background">
    <td width="75px"></td>
    <td><h3>Create new Provider Error</h3></td>
  </tr>
</table>

<% if @provider.errors.any? %>
    <h2>Invalid Assignment</h2>
    <div class='error_messages' align="left">
      <ul>
        <% for message in @provider.errors.full_messages %>
            <li><%= message %></li>
        <% end %>
      </ul>
    </div>
<% end %>

<br>
<%= link_to 'Back' , av_create_new_provider_path , :class => 'function_button menu2' %>